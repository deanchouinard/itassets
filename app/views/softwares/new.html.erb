<h1>New software</h1>

<% form_for(@software) do |f| %>
  <%= f.error_messages %>

    <p>
    <%= f.label :publisher %><br />
    <%= f.select(:publisher, @soft_pub_list) %>
  </p>
  <p>
    <%= f.label :title_version %><br />
    <%= f.text_field :title_version %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', softwares_path %>
