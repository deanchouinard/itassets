<h1>Listing computer_allocations</h1>

<table class="list-table">
  <tr>
    <th>Computer</th>
    <th>User</th>
    <th>Allocated</th>
    <th>Computer Name</th>
    <th>Returned</th>
    <th>Comments</th>
    <th> </th>
  </tr>

<% @computer_allocations.each do |computer_allocation| %>
  <tr>
    <td><%=h Computer.display_str(computer_allocation.computer_id) %></td>
    <td><%=h User.display_str(computer_allocation.user_id) %></td>
    <td><%=h computer_allocation.allocated %></td>
    <td><%=h computer_allocation.computer_name %></td>
    <td><%=h computer_allocation.returned %></td>
    <td><%=h computer_allocation.comments %></td>
    <td class="list-actions">
      <%= link_to 'Show', computer_allocation_path(computer_allocation) %><br/>
      <%= link_to 'Edit', edit_computer_allocation_path(computer_allocation) %><br/>
      <%= link_to 'Destroy', computer_allocation, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New computer_allocation', new_computer_allocation_path %>
