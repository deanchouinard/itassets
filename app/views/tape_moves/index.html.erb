<h1>Listing tape_moves</h1>

<table class="list-table">
  <tr>
    <th>Tape</th>
    <th>Destination</th>
    <th>Out</th>
    <th>Return</th>
    <th>Notes</th>
    <th />
  </tr>

<% @tape_moves.each do |tape_move| %>
  <tr>
    <td><%=h tape_move.tape.label %></td>
    <td><%=h tape_move.destination %></td>
    <td><%=h tape_move.out %></td>
    <td><%=h tape_move.return %></td>
    <td><%=h tape_move.notes %></td>
    <td><%= link_to 'Show', tape_move %></td>
    <td><%= link_to 'Edit', edit_tape_move_path(tape_move) %></td>
    <td><%= link_to 'Destroy', tape_move, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tape_move', new_tape_move_path %>