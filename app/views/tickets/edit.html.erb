<h1>Editing ticket</h1>

<% form_for(@ticket) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :computer_allocation_id %><br />
    <%= f.select(:computer_allocation_id, @computer_allocation_list) %>
  </p>
  <p>
    <%= f.label :add_user_id %><br />
    <%= f.select(:add_user_id, @user_list) %>
  </p>
  <p>
    <%= f.label :ticket_type %><br />
    <%= f.select(:ticket_type, @ticket_type_list) %>
  </p>
  <p>
    <%= f.label :contact_name %><br />
    <%= f.text_field :contact_name %>
  </p>
  <p>
    <%= f.label :contact_phone %><br />
    <%= f.text_field :contact_phone %>
  </p>
  <p>
    <%= f.label :contact_email %><br />
    <%= f.text_field :contact_email %>
  </p>
  <p>
    <%= f.label :ticket_desc %><br />
    <%= f.text_area :ticket_desc %>
  </p>
  <p>
    <%= f.label :release_date %><br />
    <%= f.date_select :release_date, :include_blank => true %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @ticket %> |
<%= link_to 'Back', tickets_path %>
