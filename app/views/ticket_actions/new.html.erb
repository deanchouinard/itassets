<h1>New ticket_action</h1>

<% form_for(@ticket_action) do |f| %>
  <%= f.error_messages %>

  <p><b>
    <%= f.label :ticket_id %><br /></b>
    <%= f.hidden_field :ticket_id %>
    <%= @ticket_display_str %>
  </p>
  <p><b>
    <%= f.label :add_user_id %><br /></b>
    <%= f.text_field :add_user_id %>
    <%= @user_display_str %>
  </p>
  <p>
    <%= f.label :action_desc %><br />
    <%= f.text_area :action_desc %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', ticket_actions_path %>
