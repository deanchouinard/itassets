<h1>Editing office</h1>
<%= form_for(@office) do |f| %>

  <div class="field">
    <%= f.label :code %><br />
    <%= f.text_field :code %>
  </div>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :company_id %><br />
    <%= f.select(:company_id, @company_list) %>
  </p>
  <p>
    <%= f.label :site_id %><br />
    <%= f.select(:site_id, @site_list) %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @office %> |
<%= link_to 'Back', offices_path %>

<%#= f.error_messages %>
