<div id="itassets-list">
<h1>Listing users</h1>
<table>
  <tr>
    <th>Name</th>
  </tr>
	<% @users.each do |user| %>
		<tr class="<%= cycle('list-line-odd', 'list-line-even') %>">

			<td class="list-description">
				<b>Login Name: </b><%=h user.name %> <br />
				<b>First Name: </b><%=h user.first_name %> <br />
				<b>Last Name: </b><%=h user.last_name %> <br />
				<b>Email: </b><%=h user.email %> <br />
				Administrator: <%=h user.admin.to_s %>
			</td>
			<td class="list-actions">
				<%= link_to 'Show', user %><br />
				<%= link_to 'Edit', edit_user_path(user) %><br />
				<%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %><br />
			</td>
		</tr>
	<% end %>
</table>

<br />

<%= link_to 'New user', new_user_path %>

