<h1>New Company</h1>

<%= form_for @company, :url => "/users/#{params[:user_id]}/companies",
  :html => {:method => :post} do |f| %>
  <%#= f.error_messages %>

  <p>
    <%= f.label :code %><br />
    <%= f.text_field :code %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', new_user_offices_path(@user) %>